<html>
  <head>
    <meta charset='utf-8'>
    <link rel='stylesheet' href='./style.css'>
    <title>Makers BnB</title>
  </head>
  <body>
  <header>
    <h1>Booking Requests</h1>
  </header>
   <div id = 'container'>
      <%@bookings.each do |booking| %>
      <div id = 'booking'>
       <br>
        <span>There is a request for the following property: <%=booking[3]%></span>
        <br>
        <span>from: <%=booking[1]%></span>
        <br>
        <span>until: <%=booking[2]%></span>
        <br>
        <span><%=booking[4]%></span>
        <br>
        <form>
          <input type=submit value='Confirm'>
        </form>
      </div>
      <%end%>
  </body>
  </html>
